	                  #statCmdBusy := FALSE;
	                  #statCmdError := FALSE;
	                  #statStatus := #STATUS_BUSY;
	                  #statFBState := #FB_BUSY;
	                  #statFBSubState := #FB_BUSY_WRITE_PARAM_977;
	                  
