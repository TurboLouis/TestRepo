	            #statReqWriteParameter.header.doId := 1;  // Drive object identification number of CU
	            #statReqWriteParameter.header.parameterNo := 1;  // Count of parameters
	            
	            // Copying the dataset into a static variable (depending on the used PLC data type)
	            #statParameter[0].index := 1;
	            #statParameter[0].parameterNo := 3101; // RTC UTC time
	            #statParameter[0].format := SINT_TO_BYTE(#FORMAT_UNSIGNED_32);
	            #statParameter[0].value := UDINT_TO_REAL(DATE_TO_UDINT(DTL_TO_DATE(#statCPUTime)) + #DATE_OFFSET); // RTC time (days); 7305 days = 20 year (between 1.1.1970 and 1.1.1990)
	            #statReqWriteParameter.data[0] := 16#10; // Attribute of parameters (0x10=Value, 0x30=Text)
	            #statReqWriteParameter.data[1] := 16#01; // No. of elements (DEC: for single elements=1) 
	            
	            // Parameter number
	            #statReqWriteParameter.data[2] := #statParameter[0].parameterNo.%B1;
	            #statReqWriteParameter.data[3] := #statParameter[0].parameterNo.%B0;
	            
	            // Subindex of parameter
	            #statReqWriteParameter.data[4] := #statParameter[0].index.%B1;
	            #statReqWriteParameter.data[5] := #statParameter[0].index.%B0;
