	            #statParameterRam2Rom[0].format := SINT_TO_BYTE(#FORMAT_UNSIGNED_16);
	            
	            #statStatus := #STATUS_BUSY;
	            #statFBState := #FB_BUSY;
	            
	            // Copying the dataset into a static variable (depending on the used PLC data type)
	            #statParameterRam2Rom[0].index := 0;
	            
