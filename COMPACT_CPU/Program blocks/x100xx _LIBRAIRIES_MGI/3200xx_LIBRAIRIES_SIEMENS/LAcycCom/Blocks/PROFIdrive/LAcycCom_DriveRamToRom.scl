	            END_IF; // #statCmdBusy = FALSE
	            
	            IF ((#statFBSubState = #FB_BUSY_WRREC_WRITE_PARAM_971) OR (#statFBSubState = #FB_BUSY_WRREC_WRITE_PARAM_977)) AND (#statError = FALSE) THEN
