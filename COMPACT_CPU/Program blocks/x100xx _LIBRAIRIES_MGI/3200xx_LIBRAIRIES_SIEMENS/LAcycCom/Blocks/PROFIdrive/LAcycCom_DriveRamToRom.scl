	                    END_CASE;
	                    
	                  END_IF;
	                  
	                END_FOR;
	                
	                IF (#statParameter[0].errorValue = 16#FF) AND (#statError = FALSE) THEN
	                  // Write p971
	                  #statCmdBusy := FALSE;
	                  #statCmdError := FALSE;
	                  #statStatus := #STATUS_BUSY;
	                  #statFBState := #FB_BUSY;
	                  #statFBSubState := #FB_BUSY_WRITE_PARAM_971;
	                  
	                ELSIF (#statParameter[1].errorValue = 16#FF)
	                  AND (#statError = FALSE)
	                THEN
