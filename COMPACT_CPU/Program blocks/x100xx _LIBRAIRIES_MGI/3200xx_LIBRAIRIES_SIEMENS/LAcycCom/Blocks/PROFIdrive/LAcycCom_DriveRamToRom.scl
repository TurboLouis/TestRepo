	              #statFBSubState := #FB_BUSY_RDREC_READ; // WRREC completed and continue with RDREC  
	              
	            ELSIF (#statCmdDone = TRUE) AND (#statError = FALSE) AND (#statFBSubState = #FB_BUSY_WRREC_WRITE_PARAM_971) THEN  // Processing completed without errors
