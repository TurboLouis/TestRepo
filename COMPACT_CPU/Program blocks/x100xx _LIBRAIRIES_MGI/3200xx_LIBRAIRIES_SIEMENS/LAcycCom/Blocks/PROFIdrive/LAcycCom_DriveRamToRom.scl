	              #statFBSubState := #FB_BUSY_RDREC_WRITE_PARAM_971; // WRREC completed and continue with RDREC
	              
	            ELSIF (#statCmdDone = TRUE) AND (#statError = FALSE) AND (#statFBSubState = #FB_BUSY_WRREC_CHECK_971) THEN  // Processing completed without errors
	              #statErrorCount := 0;  // V1.5.1 Reset error counter
	              #statFBSubState := #FB_BUSY_RDREC_CHECK_971; // WRREC completed and continue with RDREC
	              
