	            #statReqReadParameter.parameterAddress[0].parameterNo := #statParameterRam2Rom[0].parameterNo;
	            
	            // Subindex of parameter
	            #statReqReadParameter.parameterAddress[0].index := #statParameterRam2Rom[0].index;
	            
